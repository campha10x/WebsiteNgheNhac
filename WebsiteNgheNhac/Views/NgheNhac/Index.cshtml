@model Model.EF.tbl_BaiHat

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var lst_BaiHatTop = (List<Model.ViewModel.BaiHat_CaSiModel>)ViewBag.BaiHatTop;
